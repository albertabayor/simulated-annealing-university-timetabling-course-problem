   "source": [
    "---\n",
    "\n",
    "## 3. Create Initial State and Define Optimization Problem"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from timetable_sa.examples.timetabling import create_initial_state\n",
    "from timetable_sa.examples.timetabling.constraints.hard import (\n",
    "    NoRoomConflict,\n",
    "    NoLecturerConflict,\n",
    "    NoProdiConflict,\n",
    "    RoomCapacity,\n",
    "    MaxDailyPeriods,\n",
    "    FridayTimeRestriction,\n",
    "    SaturdayRestriction,\n",
    ")\n",
    "from timetable_sa.examples.timetabling.constraints.soft import (\n",
    "    Compactness,\n",
    "    PreferredRoom,\n",
    "    PrayerTimeOverlap,\n",
    ")\n",
    "from timetable_sa.examples.timetabling.moves import (\n",
    "    ChangeTimeSlot,\n",
    "    ChangeRoom,\n",
    "    SwapClasses,\n",
    "    FixRoomConflict,\n",
    "    FixLecturerConflict,\n",
    ")\n",
    "from timetable_sa.core.interfaces import SAConfig\n",
    "\n",
    "# Use subset of classes for demonstration (full dataset would take longer)\n",
    "sample_size = 30\n",
    "sample_classes = classes[:sample_size]\n",
    "\n",
    "print(f\"Using {sample_size} classes for demonstration\")\n",
    "\n",
    "# Create initial random state\n",
    "print(\"\\nCreating initial random state...\")\n",
    "initial_state = create_initial_state(\n",
    "    sample_classes,\n",
    "    rooms,\n",
    "    lecturers,\n",
    "    time_slots\n",
    ")\n",
    "\n",
    "print(f\"Initial state: {len(initial_state.schedule)} classes scheduled\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Define hard constraints\n",
    "hard_constraints = [\n",
    "    NoRoomConflict(),\n",
    "    NoLecturerConflict(),\n",
    "    NoProdiConflict(),\n",
    "    RoomCapacity(),\n",
    "    MaxDailyPeriods(),\n",
    "    FridayTimeRestriction(),\n",
    "    SaturdayRestriction(),\n",
    "]\n",
    "\n",
    "# Define soft constraints\n",
    "soft_constraints = [\n",
    "    Compactness(),\n",
    "    PreferredRoom(),\n",
    "    PrayerTimeOverlap(),\n",
    "]\n",
    "\n",
    "constraints = hard_constraints + soft_constraints\n",
    "\n",
    "# Define move generators\n",
    "move_generators = [\n",
    "    ChangeTimeSlot(),\n",
    "    ChangeRoom(),\n",
    "    SwapClasses(),\n",
    "    FixRoomConflict(),\n",
    "    FixLecturerConflict(),\n",
    "]\n",
    "\n",
    "# Configure Simulated Annealing\n",
    "config = SAConfig(\n",
    "    initial_temperature=2000.0,\n",
    "    min_temperature=0.1,\n",
    "    cooling_rate=0.995,\n",
    "    max_iterations=2000,\n",
    ")\n",
    "\n",
    "print(f\"Hard Constraints: {len(hard_constraints)}\")\n",
    "print(f\"Soft Constraints: {len(soft_constraints)}\")\n",
    "print(f\"Move Generators:  {len(move_generators)}\")\n",
    "print(f\"\\nSA Configuration:\")\n",
    "print(f\"  Initial Temperature: {config.initial_temperature}\")\n",
    "print(f\"  Cooling Rate:        {config.cooling_rate}\")\n",
    "print(f\"  Max Iterations:      {config.max_iterations}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "---\n",
    "\n",
    "## 4. Run Optimization"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from timetable_sa.examples.timetabling import SimulatedAnnealing\n",
    "\n",
    "print(\"=\"*60)\n",
    "print(\"RUNNING SIMULATED ANNEALING OPTIMIZATION\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Create SA optimizer\n",
    "sa = SimulatedAnnealing(\n",
    "    initial_state,\n",
    "    constraints,\n",
    "    move_generators,\n",
    "    config\n",
    ")\n",
    "\n",
    "# Run optimization\n",
    "start_time = time.time()\n",
    "result = sa.solve()\n",
    "end_time = time.time()\n",
    "\n",
    "execution_time = end_time - start_time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Display results\n",
    "print(f\"\\n{'='*60}\")\n",
